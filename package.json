{
  "name": "image-uploader-backend",
  "version": "1.0.0",
  "description": "The backend app to upload image files to aws s3 and insert details in aws rds",
  "main": "server.ts",
  "compilerOptions": {
    "include": [
      "app/**/*.ts"
    ],
    "exclude": [
      "node_modules"
    ]
  },
  "_moduleAliases": {
    "controllers": "app/controllers",
    "services": "app/services",
    "models": "app/models",
    "routes": "app/routes",
    "config": "app/config",
    "helpers": "app/helpers"
  },
  "scripts": {
    "start": "ts-node server.ts",
    "watch": "tsc -p tsconfig.json -w",
    "dev": "nodemon -w . --ext ts --exec ts-node server.ts",
    "build": "tsc",
    "postinstall": "npm run-script build",
    "test": "npm test"
  },
  "engines": {
    "node": "12.x"
  },
  "keywords": [
    "image-upload",
    "aws s3",
    "aws rds",
    "soa",
    "express"
  ],
  "author": "Noghan Odedra",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.7",
    "@types/node": "^14.0.27",
    "nodemon": "^2.0.3",
    "ts-node": "^8.8.2"
  },
  "dependencies": {
    "aws-sdk": "^2.726.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.9",
    "http-status-codes": "^1.4.0",
    "mime-types": "^2.1.27",
    "module-alias": "^2.2.2",
    "mysql": "^2.18.1",
    "reflect-metadata": "^0.1.13",
    "typedi": "^0.8.0",
    "typeorm": "^0.2.25",
    "typescript": "^3.8.3"
  }
}
